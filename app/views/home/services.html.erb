<h1>Usuarios</h1>
<h2>Añadir usuario</h2>
<%= form_tag("/home/services") do%>
  <%= label_tag "Nombre" %>
  <%= text_field_tag "name", nil, placeholder: "Nombre" %>
  <%= label_tag "Contraseña" %>
  <%= password_field_tag "password", nil, placeholder: "Contraseña"%>
  <%= submit_tag "Guardar" %>
<% end %>
<h2>Lista de usuarios</h2>
<% @users.each do |u| %>
<p><%= u.name%></p>
<% end %>
